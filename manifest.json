{
  "manifest_version": 2,

  "name": "Active Tab Tracker",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Active Tab Tracker"
  },
  "content_scripts": [ {
    "js": [ "jquery.min.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "http://127.0.0.1:5000/"
  ]
}